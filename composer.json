{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "aws/aws-sdk-php": "^3.336",
        "backpack/crud": "^6.8",
        "biscolab/laravel-recaptcha": "^6.1",
        "bitwasp/bitcoin": "^0.0.35",
        "btcpayserver/btcpayserver-greenfield-php": "^2.8",
        "crypto-pay/binancepay": "^1.0",
        "fpdf/fpdf": "^1.86",
        "guzzlehttp/guzzle": "^7.9",
        "hassanhelfi/number-to-arabic": "^1.0",
        "intervention/image": "^3.10",
        "jenssegers/agent": "^2.6",
        "laravel-notification-channels/webpush": "^9.0",
        "laravel/cashier": "^15.6",
        "laravel/framework": "^11.9",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "league/flysystem-aws-s3-v3": "^3.29",
        "livewire/livewire": "^3.6",
        "mpdf/mpdf": "^8.2",
        "setasign/fpdi": "^2.6",
        "setasign/fpdi-tcpdf": "^2.3",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/image": "^3.7",
        "spatie/laravel-backup": "^9.2",
        "spatie/laravel-permission": "^6.9",
        "tecnickcom/tcpdf": "^6.7",
        "yajra/laravel-datatables-oracle": "^11.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/breeze": "^2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^3.2",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
